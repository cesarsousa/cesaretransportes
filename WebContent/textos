<!-- Enviar Contato(email) -->
  <servlet>
    <servlet-name>ContatoServlet</servlet-name>
    <servlet-class>br.com.cesaretransportes.servlet.ContatoServlet</servlet-class>
    <init-param>
    	<param-name>cetrans</param-name>
    	<param-value>cesaretransportes@gmail.com</param-value>
    </init-param>
    <init-param>
    	<param-name>senha</param-name>
    	<param-value>53340276</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>ContatoServlet</servlet-name>
    <url-pattern>/enviarContato</url-pattern>
  </servlet-mapping>
  
